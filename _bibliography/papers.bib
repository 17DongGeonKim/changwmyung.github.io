	Doi = {10.1038/s41467-019-13050-3},
	Id = {Sultan2019},
	Isbn = {2041-1723},
	Journal = {Nature Communications},
	Number = {1},
	Pages = {5195},
	Title = {Superb water splitting activity of the electrocatalyst Fe3Co(PO4)4 designed with computation aid},
	Ty = {JOUR},
	Url = {https://doi.org/10.1038/s41467-019-13050-3},
	Volume = {10},
