	Isbn = {1614-6832},
	Journal = {Advanced Energy Materials},
	Journal1 = {Advanced Energy Materials},
	Journal2 = {Adv. Energy Mater.},
	Keywords = {A-site cations; charge carrier transport; electron--phonon interactions; Fr{\"o}hlich (large) polarons; lead halide perovskite solar cells},
	M3 = {https://doi.org/10.1002/aenm.201702898},
	Month = {2022/04/25},
	abstract = {As the race toward higher efficiency for inorganic/organic hybrid perovskite solar cells (PSCs) is becoming highly competitive, a design scheme to maximize carrier transport toward higher power efficiency has been urgently demanded. In this study, a hidden role of A-site cations of PSCs in carrier transport, which has been largely neglected is unraveled, i.e., tuning the Fr{\"o}hlich electron?phonon (e?ph) coupling of longitudinal optical (LO) phonon by A-site cations. The key for steering Fr{\"o}hlich polaron is to control the interaction strength and the number of proton (or lithium) coordination to halide ions. The coordination to I? alleviates electron?phonon scattering by either decreasing the Born effective charge or absorbing the LO motion of I. This novel principle discloses low electron?phonon coupling in several promising organic cations including hydroxyl?ammonium cation (NH3OH+), hydrazinium cation (NH3NH2+) and possibly Li+ solvating methylamine (Li+∙∙∙NH2CH3), on a par with methyl?ammonium cations. A new perspective on the role of A-site cations could help in improving power efficiency and accelerating the application of PSCs.},
	Number = {14},
	Pages = {1702898},
	Publisher = {John Wiley \& Sons, Ltd},
	Title = {A New Perspective on the Role of A-Site Cations in Perovskite Solar Cells},
	Ty = {JOUR},
	Url = {https://doi.org/10.1002/aenm.201702898},
	Volume = {8},
	Year = {2018},
	Year1 = {2018},
	html = {https://doi.org/10.1002/aenm.201702898},
    selected={true}
}


